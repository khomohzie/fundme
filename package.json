    "babel-plugin-styled-components": "^2.0.7",
    "dotenv": "^16.0.1",
    "ethers": "^5.6.8",
    "fs-extra": "^10.1.0",
    "ganache": "^7.2.0",
    "mocha": "^10.0.0",
    "next": "^12.1.6",
